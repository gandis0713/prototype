{
  "name": "election_test",
  "version": "1.0.0",
  "main": "dist/main.js",
  "private": true,
  "scripts": {
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "build-react": "webpack --config .webpack.conf/webpack.js",
    "build-electron": "tsc",
    "pack": "electron-builder build -w --x64=true",
    "start-electron": "electron .",
    "start-all": "yarn build-react && yarn build-electron && yarn start-electron"
  },
  "build": {
    "productName": "electron_charles",
    "appId": "electron_charles_ID",
    "copyright": "electron_charles_copyright",
    "asar": true,
    "protocols": {
      "name": "electron_charles_protocol_name",
      "schemes": [
        "electron_charles_protocol_schemes"
      ]
    },
    "win": {
      "requestedExecutionLevel": "requireAdministrator"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/electron": "^1.6.10",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "babel-loader": "^8.1.0",
    "dgram": "^1.0.1",
    "electron": "^10.1.2",
    "electron-builder": "^22.8.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "ts-loader": "^8.0.3",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@ewoosoft/ezwebserver-client": "^1.2.2164",
    "arptable-js": "^0.0.2",
    "typescript": "^4.0.2"
  }
}
